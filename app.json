{
  "expo": {
    "name": "StoryGenie",
    "slug": "StoryGenie",
    "primaryColor": "#213b55",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "facebookAppId": "1112497872824021",
    "facebookDisplayName": "StoryGenie",
    "facebookAutoLogAppEventsEnabled": true,
    "facebookAdvertiserIDCollectionEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.storygenie.storygenie",
      "buildNumber": "1.2.1",
      "supportsTablet": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "We track users for advertising purposes"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.storygenie.storygenie"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-tracking-transparency"
    ]
  }
}
